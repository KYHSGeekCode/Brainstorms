POLY *CreatePoly(unsigned char var,unsigned int maxdegree){
POLY* p=(POLY*)malloc(sizeof(POLY));
p->variable=var;
p->quotients=(*double)malloc(siseof(double)*maxdegree);
p->mul=malloc(sizeof(POLYMUL*)*maxdegree;
p->maxdegree=maxdegree;
return p;
}
